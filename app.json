{
  "expo": {
    "name": "AvocadoApp",
    "slug": "avocadoapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "scheme": "app.myavocado",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.avocado.shares.myavocado",
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.avocado.shares.myavocado"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "sentry-expo",
      "expo-router",
      "expo-localization",
      ["expo-image-picker", { "photosPermission": "Permission required to upload a new profile photo" }]
    ],
    "experiments": {
      "tsconfigPaths": true
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "avocado-zn",
            "project": "avocado"
          }
        }
      ]
    },
    "extra": {
      "eas": {
        "projectId": "9dd547c7-ad18-410b-b7bb-e9ee2a73f8ea"
      }
    },
    "owner": "avocadoadmin"
  }
}
